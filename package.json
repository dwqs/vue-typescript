  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "./node_modules/.bin/webpack --config webpack.dll.config.js",
    "dev": "./node_modules/.bin/cross-env NODE_ENV=development node ./build/dev-server.js",
    "build:stag": "./node_modules/.bin/cross-env NODE_ENV=staging ./node_modules/.bin/webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "build:pre": "./node_modules/.bin/cross-env NODE_ENV=preview ./node_modules/.bin/webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "build:prod": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "ilint": "./node_modules/.bin/tslint -p ./tsconfig.json --type-check src/**/*.ts",
    "fix": "./node_modules/.bin/tslint -p ./tsconfig.json --type-check --fix src/**/*.ts"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "vue": "^2.4.1",
    "vue-class-component": "^5.0.2",
    "vue-property-decorator": "^5.1.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.12",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.6",
    "gulp-util": "^3.0.8",
    "happypack": "^4.0.0-beta.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "less": "^2.7.2",
